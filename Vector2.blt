class Vector2 {
    decimal x;
    decimal y;

    public constructor(decimal x, decimal y) {
        this.x = x;
        this.y = y;
    }

    public static Vector2 Lerp(Vector2 a, Vector2 b, decimal rate) {
        var x = Math.Lerp(a.x, b.x, rate);
        var y = Math.Lerp(a.y, b.y, rate);
        return new Vector2(x, y);
    }
}
