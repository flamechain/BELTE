
namespace LowLevel {
    public class Int8 : int<-0x80,0x7F> {}
    public class UInt8 : int<0,0xFF> {}
    public class Int16 : int<-0x8000,0x7FFF) {}
    public class UInt16 : int<0,0xFFFF) {}
    public class Int32 : int<-0x80000000,0x7FFFFFFF> {}
    public class UInt32 : int<0,0xFFFFFFFF> {}
    public class Int64 : int<-0x8000000000000000,0x7FFFFFFFFFFFFFFF) {}
    public class UInt64 : int<0,0xFFFFFFFFFFFFFFFF) {}
}
