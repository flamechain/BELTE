{
    "files.associations": {
        "*.csproj": "msbuild",
    },
    "files.exclude": {
        "src/**/bin": true,
        "samples/**/bin": true,
        "**/obj": true,
        "**/.vs": true,
    },
    "editor.rulers": [
        80,
        120,
    ],
    "omnisharp.defaultLaunchSolution": "src/Buckle/Buckle.sln",
    "omnisharp.disableMSBuildDiagnosticWarning": true,
    "omnisharp.enableEditorConfigSupport": true,
}
